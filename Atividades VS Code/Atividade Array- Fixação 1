// 1) Criar Array com 10 itens ( de 1 a 10).
// a) Exibir no console o total de itens.
// b) Exibir no console os itens nas posições: 1, 3, 5, 8.

var materialescolar = ["caneta", "lapis", "estojo", "mochila", "borracha", "corretivo", "hidrocor", "lapiseira", "marca texto", "regua"];

console.log(materialescolar);
console.log(materialescolar.length);
console.log(materialescolar[0]);
console.log(materialescolar[2]);
console.log(materialescolar[4]);
console.log(materialescolar[7]);

// 2) Criar Array com 5 itens (nome de filmes).
// a) Exibir no console o total de itens.
// b) Exibir no console os itens nas posições: 2, 4.
// c) Exibir no console o item na primeira posição.

var filmes = ["a pequena seria", "homem de ferro", "homem de ferro 2", "aladdin", "homem de ferro 3"];

console.log(filmes.length);
console.log(filmes[1]);
console.log(filmes[3]);
console.log(filmes[0]);

// 3) Criar Array com 6 itens (nome de músicas).
// a) Exibir no console o total de itens.
// b) Exibir no console os itens nas posições: 1, 2, 6, 8.
// c) Exibir no console o item na última posição.

var musicas = [ "my power", "quem é o louco entre nos", "ai preto", "easy on me", "calm down", "love nwantiti"];

console.log(musicas.length);
console.log(musicas[0]);
console.log(musicas[1]);
console.log(musicas[5]);
console.log(musicas[7]);